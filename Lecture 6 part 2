//  part 2
#include <iostream>
using namespace std;  
int main()
{
    int i;
    i=0;
    cout<<i++<<endl;
    cout<<i;
    return 0;
}

#include <iostream>
using namespace std;  
int main()
{
    int i;
    int j=i++;
    int k=++i;
    cout<<j<<endl;
    cout<<k<<endl;
 
    return 0;
}

#include <iostream>
using namespace std;  
int main()
{

    for(int i=0;i<101;i++){
        cout<<i;
    }
    return 0;
}
