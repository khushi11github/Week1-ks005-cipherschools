 // lecture 5 part 1
#include <iostream>
using namespace std;  
int main()
{
    int a,b,c,d,e;
    cout<<"Input 5 integers as input"<<endl;
    cin>>a>>b>>c>>d>>e;
    cout<<"The integers are: "<<a<<b<<c<<d<<e;
    return 0;
}
[8:23 pm, 06/10/2023] Shavi Lpu: //lecture 5 part 2
#include <iostream>
using namespace std;  
int main()
{
    int a[4];
    cout<<"Input 5 integers as input"<<endl;
    for(int i=0;i<5;i++){
        cin>>a[i];
    }
    cout<<"The five input integers are"<<endl;
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
    return 0;
}

#include <iostream>
using namespace std;  
int main()
{
    int a[10];
    cout<<"Input 10 integers as input"<<endl;
    for(int i=0;i<10;i++){
        cin>>a[i];
    }
    cout<<"The five input integers are"<<endl;
    for(int i=9;i>=0;i--){
        cout<<a[i]<<" ";
    }
    return 0;
}
