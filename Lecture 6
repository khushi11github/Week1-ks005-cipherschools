{

    for(int i=0;i<101;i++){
        cout<<i;
    }
    return 0;
}
#include<iostream>
using namespace std;
int main(){
    int inputs;
    int i;
   for(i=1;i<101;i++){
    cin>>inputs;
    if(inputs==65){
        cout<<"Congrats you have guessed correct";
        break;
    }
   }
}
